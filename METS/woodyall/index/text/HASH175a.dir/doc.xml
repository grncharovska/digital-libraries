      &lt;hr /&gt;
      &lt;script async src=&quot;_httpextlink_&amp;amp;rl=0&amp;amp;href=%2f%2fpagead2.googlesyndication.com%2fpagead%2fjs%2fadsbygoogle.js&quot;&gt;&lt;/script&gt;
